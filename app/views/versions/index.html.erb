<% breadcrumb :versions %>
<div class="page-content">
  <div class="page-header position-relative">
    <h1>Listing versions</h1>
  </div><!--/.page-header-->

  <div class="row-fluid">
    <div class="span12">
      <table class="table table-striped table-bordered table-hover pretty">
        <thead>
  <tr>
    <th>Name</th>
    <th>Version</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  </thead>
        <tbody>

<% @versions.each do |version| %>
  <tr>
    <td><%= version.name %></td>
    <td><%= version.version %></td>
    <td><%= version.description %></td>
    <td><%= link_to 'Show', version %></td>
    <td><%= link_to 'Edit', edit_version_path(version) %></td>
    <td><%= link_to 'Destroy', version, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
        </tbody>
</table>
      <div class="digg_pagination" style="background: transparent;">
        <%= will_paginate @versions %>
      </div>

      <div class="space-4"></div>
      <%= link_to 'New Version', new_version_path ,:class => 'btn btn-primary btn-small' %>
    </div>
  </div>
</div>



